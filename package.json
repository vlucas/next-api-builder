{
  "name": "next-api-builder",
  "version": "1.0.0",
  "description": "An easy way to build API routes in Next.js with the right defaults.",
  "keywords": [
    "next",
    "nextjs",
    "api",
    "api builder",
    "next api",
    "nextjs api"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "homepage": "https://github.com/vlucas/next-api-builder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlucas/next-api-builder.git"
  },
  "bugs": {
    "url": "https://github.com/vlucas/next-api-builder/issues"
  },
  "author": "Vance Lucas",
  "license": "BSD-3-Clause",
  "scripts": {
    "lint": "eslint .",
    "test": "jest .",
    "build": "rm -rf ./build/ && tsc -b .",
    "dev": "ts-node src/index.ts",
    "prepack": "prettier -c . && npm run lint && npm run test && npm run build"
  },
  "peerDependencies": {
    "next": "*",
    "zod": "^3.*"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "next": "^13.3.0",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "zod": "^3.18.0"
